{
  "name": "yame",
  "productName": "YAME",
  "version": "0.0.1",
  "description": "A generic extensible 2D map editor for any kind of map.",
  "author": {
    "name": "Heinrich Reich",
    "email": "h.reich90@gmail.com"
  },
  "keywords": [
    "angular",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "out/electron/main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "npm run build:dev && concurrently --kill-others \"webpack --watch\" \"tsc -p src/electron/tsconfig.app.json -w\" \"npm run electron\"",
    "build:common": "tsc -p src/common/tsconfig.app.json",
    "build:electron": "tsc -p src/electron/tsconfig.app.json --outDir out && copyfiles package.json config.json out && cd out && npm install --prod && cd ..",
    "build:electron:dev": "tsc -p src/electron/tsconfig.app.json && lnfs node_modules dist/node_modules --type junction && copyfiles package.json config.json dist",
    "build": "webpack --display-error-details && npm run build:electron",
    "build:dev": "webpack --display-error-details && npm run build:electron:dev",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "editor": "npm run build:dev && npm run electron",
    "editor:prod": "npm run build:prod && npm run electron:prod",
    "electron": "electron ./dist",
    "electron:prod": "electron ./out",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:win32": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:win64": "npm run build:prod && node package.js --asar --platform=win32 --arch=x64",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "test": "karma start ./src/ng/karma.conf.js",
    "test:run": "karma run ./src/ng/karma.conf.js",
    "test:common": "jasmine-ts \"./src/common/**/*.spec.ts\"",
    "test:coverage:common": "nyc -e .ts -x \"**/*.spec.ts\" jasmine-ts \"./src/common/**/*.spec.ts\"",
    "test:electron": "jasmine-ts \"./src/electron/**/*.spec.ts\" --helper=./src/electron/test.ts",
    "test:coverage:electron": "nyc -e .ts -x \"**/*.spec.ts\" jasmine-ts \"./src/electron/**/*.spec.ts\" --helper=./src/electron/test.ts",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "dependencies": {
    "@angular/animations": "^6.0.3",
    "@angular/cdk": "^6.0.3",
    "@angular/common": "^6.0.3",
    "@angular/compiler": "^6.0.3",
    "@angular/core": "^6.0.3",
    "@angular/forms": "^6.0.3",
    "@angular/http": "^6.0.3",
    "@angular/material": "^6.0.3",
    "@angular/platform-browser": "^6.0.3",
    "@angular/platform-browser-dynamic": "^6.0.3",
    "@angular/router": "^6.0.3",
    "bluebird": "^3.5.0",
    "core-js": "2.4.1",
    "enhanced-resolve": "3.3.0",
    "eventemitter3": "^2.0.2",
    "fs-extra": "^4.0.3",
    "glob": "^7.1.2",
    "keyboardjs": "^2.4.1",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "ng2-dnd": "^5.0.2",
    "pixi.js": "^4.7.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "uuid": "^3.2.1",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.3",
    "@angular/compiler-cli": "^6.0.3",
    "@types/core-js": "0.9.36",
    "@types/fs-extra": "^4.0.2",
    "@types/glob": "^5.0.35",
    "@types/jasmine": "2.5.53",
    "@types/lodash": "^4.14.104",
    "@types/node": "^10.11.4",
    "@types/pixi.js": "^4.5.6",
    "@types/uuid": "^3.4.3",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "7.1.1",
    "awesome-typescript-loader": "^4.0.1",
    "codelyzer": "3.1.1",
    "concurrently": "^3.5.1",
    "copyfiles": "1.2.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "cssnano": "3.10.0",
    "electron": "^2.0.2",
    "electron-packager": "8.7.2",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine": "^2.6.4",
    "jasmine-core": "^2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "jasmine-ts": "^0.2.1",
    "json-loader": "0.5.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-electron": "^5.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "~2.0.3",
    "karma-webpack-grep": "^1.0.1",
    "less-loader": "4.0.4",
    "lnfs-cli": "^2.1.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "node-sass": "^4.9.3",
    "nyc": "^11.2.1",
    "postcss-loader": "1.3.3",
    "postcss-url": "7.0.0",
    "protractor": "^5.4.0",
    "raw-loader": "0.5.1",
    "sass-loader": "^7.1.0",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.1",
    "spectron": "^3.7.2",
    "style-loader": "0.18.2",
    "stylus-loader": "3.0.1",
    "ts-node": "3.1.0",
    "tslint": "5.4.3",
    "typescript": "^3.1.1",
    "url-loader": "0.5.9",
    "webdriver-manager": "^12.1.0",
    "webpack": "^3.9.0",
    "webpack-dev-server": "2.5.0",
    "webpack-node-externals": "^1.6.0"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "typings": "./typings.d.ts"
}
