{
  "name": "yame",
  "productName": "YAME",
  "version": "0.0.1",
  "description": "A generic extensible 2D map editor for any kind of map.",
  "author": {
    "name": "Heinrich Reich",
    "email": "h.reich90@gmail.com"
  },
  "keywords": [
    "angular",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "./main/main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "npm run build && npm run watch",
    "watch": "concurrently --kill-others \"webpack --mode development --watch\" \"tsc -p src/main/tsconfig.app.json -w\" \"npm run electron\"",
    "build:common": "tsc -p src/common/tsconfig.app.json",
    "build:electron": "tsc -p src/main/tsconfig.app.json && lnfs node_modules out/node_modules --type junction && copyfiles package.json config.json out",
    "build:electron:prod": "tsc -p src/main/tsconfig.app.json --outDir dist && copyfiles package.json config.json dist && cd dist && npm install --prod && cd ..",
    "build": "ng build --outputPath=out/ng && npm run build:electron",
    "build:prod": "ng build --prod --outputPath=dist/ng && npm run build:electron:prod",
    "editor": "npm run build && npm run electron",
    "editor:prod": "npm run build:prod && npm run electron:prod",
    "electron": "electron ./out",
    "electron:prod": "electron ./dist",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:win32": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:win64": "npm run build:prod && node package.js --asar --platform=win32 --arch=x64",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "test": "npm run test:common && npm run test:electron && npm run test:ng",
    "test:ng": "ng test",
    "test:common": "jasmine-ts \"./src/common/**/*.spec.ts\"",
    "test:coverage:common": "nyc -e .ts -x \"**/*.spec.ts\" jasmine-ts \"./src/common/**/*.spec.ts\"",
    "test:electron": "jasmine-ts \"./src/main/**/*.spec.ts\" --helper=./src/main/test.ts",
    "test:coverage:electron": "nyc -e .ts -x \"**/*.spec.ts\" jasmine-ts \"./src/main/**/*.spec.ts\" --helper=./src/main/test.ts",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js",
    "ng:watch": "ng build --outputPath=out/ng --watch"
  },
  "dependencies": {
    "@trixt0r/ecs": "^0.4.1",
    "bluebird": "^3.5.0",
    "eventemitter3": "^3.1.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.2",
    "lodash": "^4.17.20",
    "pixi.js": "^5.3.0",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.3",
    "@angular/animations": "^11.0.9",
    "@angular/cdk": "^11.0.1",
    "@angular/cli": "^11.0.9",
    "@angular/common": "^11.0.9",
    "@angular/compiler": "^11.0.9",
    "@angular/compiler-cli": "^11.0.9",
    "@angular/core": "^11.0.9",
    "@angular/forms": "^11.0.9",
    "@angular/material": "^11.0.9",
    "@angular/platform-browser": "^11.0.9",
    "@angular/platform-browser-dynamic": "^11.0.9",
    "@angular/router": "^11.0.9",
    "@iplab/ngx-color-picker": "^1.0.5",
    "@ng-select/ng-select": "^5.0.9",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "@ngxs/store": "^3.7.1",
    "@types/bluebird": "^3.5.33",
    "@types/electron": "^1.6.10",
    "@types/fs-extra": "^8.1.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "@yame/dev": "git+https://github.com/Trixt0r/yame-dev.git#npm",
    "angular-pipes": "^10.0.0",
    "angular-tree-component": "^8.5.6",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "7.1.1",
    "awesome-typescript-loader": "^5.2.0",
    "clean-webpack-plugin": "^0.1.19",
    "codelyzer": "^6.0.0",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^6.3.2",
    "copyfiles": "^1.2.0",
    "core-js": "2.4.1",
    "cross-env": "5.0.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "electron": "^11.0.3",
    "electron-packager": "8.7.2",
    "enhanced-resolve": "3.3.0",
    "exports-loader": "0.6.4",
    "file-loader": "^2.0.0",
    "hammerjs": "^2.0.8",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "^3.2.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine": "^2.6.4",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jasmine-ts": "^0.2.1",
    "json-loader": "0.5.4",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-electron": "^5.2.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "~2.0.3",
    "karma-webpack-grep": "^1.0.1",
    "lnfs-cli": "^2.1.0",
    "mkdirp": "^1.0.4",
    "ng2-dnd": "^5.0.2",
    "nyc": "^14.1.1",
    "protractor": "~7.0.0",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.4",
    "rxjs-compat": "^6.5.4",
    "sass": "^1.29.0",
    "spectron": "^13.0.0",
    "ts-node": "3.1.0",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "^4.0.5",
    "url-loader": "^2.1.0",
    "webdriver-manager": "^12.1.0",
    "webpack": "^5.17.0",
    "zone.js": "^0.10.3"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "typings": "./typings.d.ts",
  "browser": {
    "fs": false,
    "fs-extra": false,
    "path": false,
    "constants": false,
    "electron": false,
    "module": "module"
  }
}
