{
  "name": "yame",
  "productName": "YAME",
  "version": "0.0.1",
  "description": "A generic extensible 2D map editor for any kind of map.",
  "author": {
    "name": "Heinrich Reich",
    "email": "h.reich90@gmail.com"
  },
  "keywords": [
    "angular",
    "electron",
    "typescript",
    "sass"
  ],
  "main": "electron/main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "webpack --watch",
    "start:web": "webpack-dev-server --content-base . --port 4200 --inline",
    "build:common": "tsc -p src/common/tsconfig.app.json",
    "build:electron": "tsc -p src/electron/tsconfig.app.json && copyfiles package.json dist && cd dist && npm install --prod && cd ..",
    "build:electron:dev": "tsc -p src/electron/tsconfig.app.json && lnfs node_modules dist/node_modules --type dir && copyfiles package.json dist",
    "build": "webpack --display-error-details && npm run build:electron",
    "build:dev": "webpack --display-error-details && npm run build:electron:dev",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "app": "npm run build && electron ./dist",
    "electron:serve": "npm run build:electron && electron ./dist --serve",
    "electron": "electron ./dist",
    "electron:dev": "npm run build:electron && electron ./dist",
    "electron:prod": "npm run build:prod && electron ./dist",
    "electron:linux": "npm run build:prod && node package.js --asar --platform=linux --arch=x64",
    "electron:win32": "npm run build:prod && node package.js --asar --platform=win32 --arch=ia32",
    "electron:win64": "npm run build:prod && node package.js --asar --platform=win32 --arch=x64",
    "electron:mac": "npm run build:prod && node package.js --asar --platform=darwin --arch=x64",
    "test": "karma start ./src/ng/karma.conf.js",
    "test:run": "karma run ./src/ng/karma.conf.js",
    "test:common": "jasmine-ts \"./src/common/**/*.spec.ts\"",
    "test:coverage:common": "nyc -e .ts -x \"**/*.spec.ts\" jasmine-ts \"./src/common/**/*.spec.ts\"",
    "test:electron": "jasmine-ts \"./src/electron/**/*.spec.ts\" --helper=./src/electron/test.ts",
    "test:coverage:electron": "nyc -e .ts -x \"**/*.spec.ts\" jasmine-ts \"./src/electron/**/*.spec.ts\" --helper=./src/electron/test.ts",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js"
  },
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "bluebird": "^3.5.0",
    "core-js": "2.4.1",
    "enhanced-resolve": "3.3.0",
    "eventemitter3": "^2.0.2",
    "fs-extra": "^4.0.3",
    "keyboardjs": "^2.4.1",
    "lodash": "^4.17.5",
    "material-design-icons": "^3.0.1",
    "ng2-dnd": "^5.0.2",
    "pixi.js": "^4.7.0",
    "rxjs": "^5.2.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.3.1",
    "@angular/compiler-cli": "^5.2.0",
    "@types/core-js": "0.9.36",
    "@types/fs-extra": "^4.0.2",
    "@types/jasmine": "2.5.53",
    "@types/lodash": "^4.14.104",
    "@types/node": "7.0.7",
    "@types/pixi.js": "^4.5.6",
    "autoprefixer": "7.1.1",
    "codelyzer": "3.1.1",
    "copyfiles": "1.2.0",
    "cross-env": "5.0.1",
    "css-loader": "0.28.4",
    "cssnano": "3.10.0",
    "electron": "^1.8.2",
    "electron-packager": "8.7.2",
    "electron-reload": "1.2.1",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.2",
    "html-loader": "0.4.5",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "jasmine-ts": "^0.2.1",
    "json-loader": "0.5.4",
    "karma": "1.7.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.0",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-sourcemap-loader": "0.3.7",
    "less-loader": "4.0.4",
    "lnfs-cli": "^2.1.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "nyc": "^11.2.1",
    "postcss-loader": "1.3.3",
    "postcss-url": "7.0.0",
    "protractor": "5.1.2",
    "raw-loader": "0.5.1",
    "sass-loader": "6.0.6",
    "script-loader": "0.7.0",
    "source-map-loader": "0.2.1",
    "spectron": "^3.7.2",
    "style-loader": "0.18.2",
    "stylus-loader": "3.0.1",
    "ts-node": "3.1.0",
    "tslint": "5.4.3",
    "typescript": "2.5.0",
    "url-loader": "0.5.9",
    "webdriver-manager": "12.0.6",
    "webpack": "3.3.0",
    "webpack-dev-server": "2.5.0"
  },
  "license": "SEE LICENSE IN LICENSE.md"
}
